<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

	<servlet>
		<servlet-name>Bimatrix</servlet-name>
		<servlet-class>lse.math.games.web.BimatrixServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Tree</servlet-name>
		<servlet-class>lse.math.games.web.TreeServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LrsTest</servlet-name>
		<servlet-class>lse.math.games.web.LrsTestServlet</servlet-class>
	</servlet>	
	<servlet>
		<servlet-name>LrsC</servlet-name>
		<servlet-class>lse.math.games.web.LrsCServlet</servlet-class>
	</servlet>	
	<servlet>
		<servlet-name>Native</servlet-name>
		<servlet-class>lse.math.games.web.NativeServlet</servlet-class>
	</servlet>		

	
	<servlet-mapping>
		<servlet-name>Native</servlet-name>
		<url-pattern>/native/servlet</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>LrsC</servlet-name>
		<url-pattern>/lrsc/testservlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>LrsC</servlet-name>
		<url-pattern>/lrsc/servlet</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Bimatrix</servlet-name>
		<url-pattern>/matrix/servlet</url-pattern>
	</servlet-mapping>	
	
	<servlet-mapping>
		<servlet-name>Tree</servlet-name>
		<url-pattern>/tree/servlet</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
